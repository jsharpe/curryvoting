<h1>Listing votes</h1>

<table>
  <tr>
  </tr>
<% @votes.each do |vote| %>
  <tr>
    <td> <%= h vote.curryhouse.map(&:title)
	%></td>
    <td><%= link_to 'Show', [@event, vote] %></td>
    <td><%= link_to 'Edit', edit_event_vote_path(@event,vote) %></td>
    <td><%= link_to 'Destroy', [@event,vote], :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New vote', new_event_vote_path(@event) %>
